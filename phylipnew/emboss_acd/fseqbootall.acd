application: fseqbootall [
  documentation: "Bootstrapped sequences algorithm"
  groups: "Phylogeny:Molecular sequence"
  embassy: "phylipnew"
  relations:  "EDAM:0000084 topic Phylogenetics"
  relations:  "EDAM:0000552 operation Phylogenetic tree bootstrapping"
]

section: input [
  information: "Input section"
  type: "page"
]

  seqset: infilesequences [
    parameter: "Y"
    type: "gapany"
    aligned: "Y"
    relations: "EDAM:0000863 data Sequence alignment"
  ]

  properties: categories [
    additional: "Y"
    characters: ""
    information: "File of input categories"
    nullok: "Y"
    size: "1"
    length: "$(infile.length)"
    relations: "EDAM:0001427 data Phylogenetic discrete data"
  ]

  properties: mixfile [
    additional: "Y"
    characters: ""
    information: "File of mixtures"
    nullok: "Y"
    size: "1"
    length: "$(infile.length)"
    relations: "EDAM:0001427 data Phylogenetic discrete data"
  ]

  properties: ancfile [
    additional: "Y"
    characters: ""
    information: "File of ancestors"
    nullok: "Y"
    size: "1"
    length: "$(infile.length)"
    relations: "EDAM:0001427 data Phylogenetic discrete data"
  ]

  properties: weights [
    additional: "Y"
    characters: "01"
    information: "Weights file"
    help: "Weights file"
    length: "$(infile.length)"
    nullok: "Y"
    relations: "EDAM:0002994 data Phylogenetic character weights"
  ]

  properties: factorfile [
    additional: "Y"
    information: "Factors file"
    nullok: "Y"
    length: "$(infile.length)"
    size: "1"
    relations: "EDAM:0001427 data Phylogenetic discrete data"
  ]

endsection: input

section: additional [
  information: "Additional section"
  type: "page"
]

  list: datatype [
    additional: "y"
    minimum: "1"
    maximum: "1"
    header: "Datatype"
    values: "s:Molecular sequences; m:Discrete Morphology;
             r:Restriction Sites; g:Gene Frequencies"
    information: "Choose the datatype"
    default: "s"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  list: test [
    additional: "y"
    minimum: "1"
    maximum: "1"
    header: "Test"
    values: "b:Bootstrap; j:Jackknife; c:Permute species for each
             character; o:Permute character order; s:Permute within species;
             r:Rewrite data"
    information: "Choose test"
    default: "b"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  toggle: regular [
    additional: "@( $(test) == { b | j } )"
    information: "Altered sampling fraction"
    default: "N"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  float: fracsample [
    additional: "@(!$(regular))"
    information: "Samples as percentage of sites"
    default: "100.0"
    minimum: "0.1"
    maximum: "100.0"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  list: rewriteformat [
    additional: "@(@($(test)==r)&&@($(datatype)==s))"
    minimum: "1"
    maximum: "1"
    header: "test"
    values: "p:PHYLIP; n:NEXUS; x:XML"
    information: "Output format"
    default: "p"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  list: seqtype [
    additional: "@( @( $(datatype) == s ) & @( $(rewriteformat)
                 == {n | x} ))"
    minimum: "1"
    maximum: "1"
    header: "test"
    values: "d:dna; p:protein; r:rna"
    information: "Output format"
    default: "d"
    relations: "EDAM:0001094 data Sequence type"
  ]

  list: morphseqtype [
    additional: "@( @( $(datatype) == m ) & @( $(test) == r ))"
    minimum: "1"
    maximum: "1"
    header: "Output format"
    values: "p:PHYLIP; n:NEXUS"
    information: "Output format"
    default: "p"
    relations: "EDAM:0002129 identifier File format name"
  ]

  integer: blocksize [
    information: "Block size for bootstraping"
    additional: "@($(test) == b)"
    default: "1"
    minimum: "1"
    relations: "EDAM:0001249 data Sequence length"
  ]

  integer: reps [
    additional: "@($(test) != r)"
    information: "How many replicates"
    minimum: "1"
    default: "100"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  list: justweights [
    additional: "@( $(test) == { b | j } )"
    minimum: "1"
    maximum: "1"
    header: "Write out datasets or just weights"
    values: "d:Datasets; w:Weights"
    information: "Write out datasets or just weights"
    default: "d"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  boolean: enzymes [
    additional: "@($(datatype) == r)"
    information: "Is the number of enzymes present in input
                  file"
    default: "N"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  boolean: all [
    additional: "@($(datatype) == g)"
    information: "All alleles present at each locus"
    default: "N"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  integer: seed [
    additional: "@($(test) != r)"
    information: "Random number seed between 1 and 32767 (must
                  be odd)"
    minimum: "1"
    maximum: "32767"
    default: "1"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

endsection: additional

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "seqboot output"
    information: "Phylip seqboot program output file"
    relations:  "EDAM:0002245 data Sequence set (bootstrapped)"
  ]

  boolean: printdata [
    additional: "Y"
    default: "N"
    information: "Print out the data at start of run"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  boolean: dotdiff [
    additional: "$(printdata)"
    default: "Y"
    information: "Use dot-differencing"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  boolean: progress [
    additional: "Y"
    default: "Y"
    information: "Print indications of progress of run"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

endsection: output
