application: fdnamove [
  documentation: "Interactive DNA parsimony"
  groups: "Phylogeny:Molecular sequence"
  embassy: "phylipnew"
  relations:  "EDAM:0000191 topic Phylogeny reconstruction"
  relations:  "EDAM:0000545 operation Phylogenetic tree construction
                                      (parsimony methods)"
]

section: input [
  information: "Input section"
  type: "page"
]

  seqsetall: sequence [
    parameter: "Y"
    type: "gapdnaphylo"
    aligned: "Y"
    relations: "EDAM:0001383 data Sequence alignment (nucleic acid)"
  ]

  tree: intreefile [
    parameter: "Y"
    nullok: "Y"
    knowntype: "newick"
    information: "Phylip tree file (optional)"
    relations:  "EDAM:0000872 data Phylogenetic tree"
  ]

  properties: weights [
    additional: "Y"
    characters: "01"
    information: "Phylip weights file (optional)"
    length: "$(sequence.length)"
    size: ""
    help: "Weights file - ignore sites with weight zero"
    nullok: "Y"
    relations: "EDAM:0002994 data Phylogenetic character weights"
  ]

endsection: input

section: additional [
  information: "Additional section"
  type: "page"
]

  integer: outgrno [
    additional: "Y"
    minimum: "0"
    maximum: "$(sequence.count)"
    failrange: "N"
    trueminimum: "Y"
    default: "0"
    information: "Species number to use as outgroup"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  toggle: thresh [
    additional: "Y"
    default: "N"
    information: "Use threshold parsimony"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  float: threshold [
    additional: "$(thresh)"
    minimum: "1"
    information: "Threshold value"
    default: "1"
    relations: "EDAM:0002146 data Threshold"
  ]

  list: initialtree [
    additional: "Y"
    minimum: "1"
    maximum: "1"
    header: "Initial tree"
    values: "a:Arbitary; u:User; s:Specify"
    information: "Initial tree"
    default: "Arbitary"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  integer: screenwidth [
    additional: "Y"
    default: "80"
    information: "Width of terminal screen in characters"
    relations: "EDAM:0002152 data Rendering parameter"
  ]

  integer: screenlines [
    additional: "Y"
    default: "24"
    information: "Number of lines on screen"
    relations: "EDAM:0002152 data Rendering parameter"
  ]

endsection: additional

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outtreefile [
    additional: "Y"
    extension: "treefile"
    knowntype: "newick tree"
    information: "Phylip tree output file (optional)"
    nullok: "Y"
    relations:  "EDAM:0000872 data Phylogenetic tree"
  ]

endsection: output
