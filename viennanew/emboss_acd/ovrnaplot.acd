application: ovrnaplot [
  documentation: "Plot vrnafold output"
  groups: "Nucleic:RNA folding, Nucleic:2D structure"
  gui: "yes"
  batch: "yes"
  cpu: "medium"
  embassy: "vienna"
  relations: "EDAM:0000697 topic RNA structure and alignment"
  relations: "EDAM:0000337 operation Plotting and rendering"
  relations: "EDAM:0000568 operation RNA secondary structure rendering"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: structuresfile [
    parameter: "Y"
    information: "Vienna RNAfold output file"
    knowntype: "rnafold output"
    relations:  "EDAM:0000880 data RNA secondary structure"
  ]

endsection: input

section: advanced [
  information: "Advanced section"
  type: "page"
]

  list: layout [
    default: "naview"
    minimum: "1"
    maximum: "1"
    header: "Energy parameters"
    values: "radial:Simple radial, naview:naview"
    delimiter: ","
    codedelimiter: ":"
    information: "Layout"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  list: optype [
    default: "ps"
    minimum: "1"
    maximum: "1"
    header: "Output type"
    values: "ps:postscript, gml:graph meta language, svg:scaleable
             vector graphics, xrna:XRNA save file"
    delimiter: ","
    codedelimiter: ":"
    information: "Type"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  string: pre [
    information: "Pre-annotation"
    default: ""
    knowntype: "string"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

  string: post [
    information: "Post-annotation"
    default: ""
    knowntype: "string"
    relations: "EDAM:0002527 data Parameter or primitive"
  ]

endsection: advanced

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "rnafold output"
    information: "Vienna RNAfold output file"
    relations:  "EDAM:0000880 data RNA secondary structure"
  ]

endsection: output
